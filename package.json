{
  "name": "tensorflow-js-website",
  "version": "0.0.1",
  "private": true,
  "hexo": {
    "version": "3.6.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "prep": "git submodule init && git submodule update && git submodule foreach git pull origin master && yarn",
    "serve": "hexo clean && hexo server",
    "build": "hexo clean && hexo generate",
    "post-build": "babel public/js/layout.js -o public/js/layout.js && babel public/js/api.js -o public/js/api.js",
    "build-api": "hexo clean && node build-scripts/build-api.js --local",
    "build-and-serve": "yarn build-api && yarn serve",
    "build-prod": "yarn prep && hexo clean && node build-scripts/build-api.js && yarn build && yarn post-build"
  },
  "dependencies": {
    "@tensorflow/tfjs": "0.8.0",
    "babel-cli": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "commander": "~2.14.1",
    "hexo": "~3.6.0",
    "hexo-browsersync": "^0.3.0",
    "hexo-generator-archive": "~0.1.4",
    "hexo-generator-category": "~0.1.3",
    "hexo-generator-tag": "~0.2.0",
    "hexo-renderer-ejs": "~0.3.0",
    "hexo-renderer-handlebars": "~2.0.2",
    "hexo-renderer-markdown-it": "^3.4.1",
    "hexo-renderer-sass": "~0.3.2",
    "hexo-renderer-stylus": "~0.3.1",
    "hexo-server": "~0.2.0",
    "highlight.js": "^9.12.0",
    "markdown-it": "^8.4.1",
    "material-components-web": "^0.32.0",
    "mkdirp": "~0.5.1",
    "semver-compare": "^1.0.0",
    "shelljs": "~0.8.1",
    "ts-node": "~4.1.0",
    "typescript": "~2.7.2"
  },
  "devDependencies": {
    "@types/node": "~9.4.6",
    "clang-format": "~1.2.2"
  }
}
