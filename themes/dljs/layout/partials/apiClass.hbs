<div class="symbol class">
  <div class="symbol-header">
    <a class="symbol-link" name="{{urlHash}}" href="#{{urlHash}}">
      {{~displayName~}}
    </a>
    <span class="signature">
      {{#if inheritsFrom}}
        <span>extends {{{markdownInner inheritsFrom}}}</span>
      {{/if}}
    </span>
    <span class="symbol-marker">class</span>
    <span class="source-link">
      <a href="{{githubUrl}}" target=_blank>Source</a>
    </span>
  </div>

  <div class="documentation">{{{markdown documentation}}}</div>

  {{!-- Method List --}}
  <div class="method-list">
    {{#methods}}
      {{> apiFunction this isMethod=true}}
    {{/methods}}
  </div>
</div>
